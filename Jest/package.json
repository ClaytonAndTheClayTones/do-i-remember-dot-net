{
  "name": "@veho/qdk",
  "version": "0.0.0-development",
  "description": "SDK for generating test data in use for e2e and integration testing",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "rm -rf dist/* && tsc --project tsconfig.prod.json && cp package*.json README.md dist/",
    "prepare": "husky install",
    "lint": "npm-run-all --parallel lint:check:*",
    "lint:check:eslint": "eslint --ext .js,.ts .",
    "lint:check:prettier": "prettier --ignore-path .gitignore --check \"./**/*.{ts,js,jsx,tsx,json,graphql,yaml}\"",
    "lint:check:tsc": "tsc --noEmit",
    "lint:fix": "npm-run-all --parallel lint:fix:*",
    "lint:fix:eslint": "eslint --ext .js,.ts . --fix",
    "lint:fix:prettier": "prettier --ignore-path .gitignore --write \"./**/*.{ts,js,jsx,tsx,json,graphql,yaml}\"",
    "fetchSchema:dev": "rover graph fetch hermes-es7mvc@dev > schema.graphql",
    "fetchSchema:staging": "rover graph fetch hermes-es7mvc@staging > schema.graphql",
    "fetchSchema:prod": "rover graph fetch hermes-es7mvc@prod > schema.graphql",
    "codegen": "graphql-codegen --config codegen.yml",
    "postcodegen": "npm run lint:fix:prettier",
    "codegen:dev": "npm run fetchSchema:dev && npm run codegen",
    "codegen:staging": "npm run fetchSchema:dev && npm run codegen",
    "codegen:prod": "npm run fetchSchema:dev && npm run codegen",
    "release": "sh scripts/release.sh",
    "semantic-release": "semantic-release",
    "generate": "npm-run-all --parallel generate:*",
    "generate:docs": "npx typedoc --out docs/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@apollo/rover": "^0.9.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.7",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.8.0",
    "@graphql-codegen/typescript-resolvers": "^2.7.5",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.2.0",
    "@types/luxon": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@zamiell/typedoc-plugin-not-exported": "^0.2.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.27.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.2",
    "jest": "^29.4.3",
    "jest-expect-message": "^1.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "semantic-release": "^19.0.5",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.23",
    "typedoc-plugin-markdown": "^3.14.0",
    "typedoc-plugin-missing-exports": "^1.0.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": "^16.14.2",
    "npm": "^8.5.0"
  },
  "dependencies": {
 
  }
}
